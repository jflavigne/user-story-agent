{
  "name": "user-story-agent",
  "version": "0.1.0",
  "type": "module",
  "description": "Create user stories from mockups and designs",
  "repository": {
    "type": "git",
    "url": "https://github.com/jflavigne-sidlee/user-story-agent.git"
  },
  "scripts": {
    "download-fixture": "node scripts/download-figma-fixture.mjs",
    "build": "tsc",
    "benchmark": "tsx scripts/benchmark.ts",
    "generate-skills": "tsx src/skills/generate.ts",
    "agent": "tsx src/cli.ts",
    "dev": "tsx watch src/agent/index.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:vision": "vitest run tests/unit tests/contract tests/integration",
    "test:vision:live": "RUN_LIVE_VISION_TESTS=1 vitest run tests/live"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "sharp": "^0.33.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "~5.7.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ]
}
